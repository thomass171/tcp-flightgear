<?xml version="1.0"?>
<!--== Bluebird Explorer Hovercraft  version 8.6 ==-->

<PropertyList>

 <path>gps2.ac</path>

 <animation>
	<name>gps-lit</name>
	<object-name>gps.label</object-name>
	<object-name>lat.102</object-name>
	<object-name>lat.103</object-name>
	<object-name>lat.104</object-name>
	<object-name>lat.105</object-name>
	<object-name>lat.106</object-name>
	<object-name>lat.107</object-name>
	<object-name>lon.101</object-name>
	<object-name>lon.102</object-name>
	<object-name>lon.103</object-name>
	<object-name>lon.104</object-name>
	<object-name>lon.105</object-name>
	<object-name>lon.106</object-name>
	<object-name>lon.107</object-name>
	<object-name>gps.dec</object-name>
	<object-name>gps.dmm</object-name>
	<object-name>gps.dms</object-name>
	<object-name>gps.north</object-name>
	<object-name>gps.south</object-name>
	<object-name>gps.west</object-name>
	<object-name>gps.east</object-name>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.dec</object-name>
	<condition>
		<equals>
			<property>sim/lon-lat-format</property>
			<value>0</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.dmm</object-name>
	<condition>
		<equals>
			<property>sim/lon-lat-format</property>
			<value>1</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.dms</object-name>
	<condition>
		<equals>
			<property>sim/lon-lat-format</property>
			<value>2</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.north</object-name>
	<condition>
		<greater-than>
			<property>position/latitude-deg</property>
			<value>0</value>
		</greater-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.south</object-name>
	<condition>
		<less-than>
			<property>position/latitude-deg</property>
			<value>0</value>
		</less-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.west</object-name>
	<condition>
		<less-than>
			<property>position/longitude-deg</property>
			<value>0</value>
		</less-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>gps.east</object-name>
	<condition>
		<greater-than>
			<property>position/longitude-deg</property>
			<value>0</value>
		</greater-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>lat.102</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>position/latitude-deg</property>
				<value>10.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>position/latitude-deg</property>
				<value>-10.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>lon.101</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>position/longitude-deg</property>
				<value>100.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>position/longitude-deg</property>
				<value>-100.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>lon.102</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>position/longitude-deg</property>
				<value>10.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>position/longitude-deg</property>
				<value>-10.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lat.102</object-name>
	<property>instrumentation/digital/lat-whole</property>
	<factor>0.01</factor>
	<step>10</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lat.103</object-name>
	<property>instrumentation/digital/lat-whole</property>
	<factor>0.1</factor>
	<step>1</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lat.104</object-name>
	<property>instrumentation/digital/lat-fraction</property>
	<factor>0.0001</factor>
	<step>1000</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

