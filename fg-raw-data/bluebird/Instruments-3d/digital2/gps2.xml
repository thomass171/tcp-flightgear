	<type>textranslate</type>
	<object-name>lat.106</object-name>
	<property>instrumentation/digital/lat-fraction</property>
	<factor>0.01</factor>
	<step>10</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lat.107</object-name>
	<property>instrumentation/digital/lat-fraction</property>
	<factor>0.1</factor>
	<step>1</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.101</object-name>
	<property>instrumentation/digital/lon-whole</property>
	<factor>0.001</factor>
	<step>100</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.102</object-name>
	<property>instrumentation/digital/lon-whole</property>
	<factor>0.01</factor>
	<step>10</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.103</object-name>
	<property>instrumentation/digital/lon-whole</property>
	<factor>0.1</factor>
	<step>1</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.104</object-name>
	<property>instrumentation/digital/lon-fraction</property>
	<factor>0.0001</factor>
	<step>1000</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.105</object-name>
	<property>instrumentation/digital/lon-fraction</property>
	<factor>0.001</factor>
	<step>100</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.106</object-name>
	<property>instrumentation/digital/lon-fraction</property>
	<factor>0.01</factor>
	<step>10</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>lon.107</object-name>
