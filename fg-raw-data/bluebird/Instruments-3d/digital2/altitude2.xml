 <animation>
	<type>select</type>
	<object-name>agl.104</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>instrumentation/digital/agl-whole</property>
				<value>100.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>instrumentation/digital/agl-whole</property>
				<value>-100.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>agl.105</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>instrumentation/digital/agl-whole</property>
				<value>10.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>instrumentation/digital/agl-whole</property>
				<value>-10.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>textranslate</type>
	<object-name>alt.101</object-name>
	<property>instrumentation/digital/altitude-whole</property>
	<factor>0.000001</factor>
	<step>100000</step>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
 </animation>

