		<greater-than-equals>
			<property>instrumentation/digital/agl-whole</property>
			<value>100000.0</value>
		</greater-than-equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>agl.102</object-name>
	<condition>
		<greater-than-equals>
			<property>instrumentation/digital/agl-whole</property>
			<value>10000.0</value>
		</greater-than-equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>agl.103</object-name>
	<object-name>agl.comma</object-name>
	<condition>
		<or>
			<greater-than-equals>
