<?xml version="1.0"?>
<!--== Bluebird Explorer Hovercraft  version 8.6 ==-->

<PropertyList>

 <path>altitude2.ac</path>

 <animation>
	<name>altitude-lit</name>
	<object-name>alt.label</object-name>
	<object-name>alt.minus</object-name>
	<object-name>alt.101</object-name>
	<object-name>alt.102</object-name>
	<object-name>alt.103</object-name>
	<object-name>alt.104</object-name>
	<object-name>alt.105</object-name>
	<object-name>alt.106</object-name>
	<object-name>agl.minus</object-name>
	<object-name>agl.101</object-name>
	<object-name>agl.102</object-name>
	<object-name>agl.103</object-name>
	<object-name>agl.104</object-name>
	<object-name>agl.105</object-name>
	<object-name>agl.106</object-name>
	<object-name>alt.comma</object-name>
	<object-name>agl.comma</object-name>
	<object-name>alt.water</object-name>
	<object-name>alt.agl</object-name>
	<object-name>alt.elev</object-name>
	<object-name>alt.ft</object-name>
	<object-name>alt.m</object-name>
	<object-name>alt.warn1</object-name>
	<object-name>alt.warn2</object-name>
	<object-name>alt.warn3</object-name>
	<object-name>agl.hold.yellow</object-name>
	<object-name>alt.hold.red</object-name>
	<object-name>alt.hold.yellow</object-name>
 </animation>

 <animation>
	<type>select</type>
	<object-name>altitude-lit</object-name>
	<condition>
		<property>sim/model/bluebird/systems/power-switch</property>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.101</object-name>
	<condition>
		<greater-than-equals>
			<property>instrumentation/digital/altitude-whole</property>
			<value>100000.0</value>
		</greater-than-equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.102</object-name>
	<condition>
		<greater-than-equals>
			<property>instrumentation/digital/altitude-whole</property>
			<value>10000.0</value>
		</greater-than-equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.103</object-name>
	<object-name>alt.comma</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>instrumentation/digital/altitude-whole</property>
				<value>1000.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>instrumentation/digital/altitude-whole</property>
				<value>-1000.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.104</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>instrumentation/digital/altitude-whole</property>
				<value>100.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>instrumentation/digital/altitude-whole</property>
				<value>-100.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.105</object-name>
	<condition>
		<or>
			<greater-than-equals>
				<property>instrumentation/digital/altitude-whole</property>
				<value>10.0</value>
			</greater-than-equals>
			<less-than-equals>
				<property>instrumentation/digital/altitude-whole</property>
				<value>-10.0</value>
			</less-than-equals>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>agl.101</object-name>
