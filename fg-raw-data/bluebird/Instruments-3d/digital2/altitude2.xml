 <animation>
	<type>select</type>
	<object-name>alt.minus</object-name>
	<condition>
		<less-than>
			<property>position/altitude-ft</property>
			<value>0</value>
		</less-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>agl.minus</object-name>
	<condition>
		<less-than>
			<property>sim/model/bluebird/position/altitude-agl-ft</property>
			<value>0</value>
		</less-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.ft</object-name>
	<condition>
		<equals>
			<property>instrumentation/digital/altitude-mode</property>
			<value>0</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.m</object-name>
	<condition>
		<equals>
			<property>instrumentation/digital/altitude-mode</property>
			<value>1</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.warn1</object-name>
	<condition>
		<property>sim/model/bluebird/lighting/ground-near</property>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.warn2</object-name>
	<condition>
		<equals>
			<property>sim/model/bluebird/lighting/ground-warning</property>
			<value>1</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.warn3</object-name>
	<condition>
		<equals>
			<property>sim/model/bluebird/lighting/ground-warning</property>
			<value>2</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.water</object-name>
	<condition>
		<less-than-equals>
			<property>position/ground-elev-ft</property>
			<value>0</value>
		</less-than-equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.hold.red</object-name>
	<condition>
		<and>
			<not>
				<equals>
					<property>autopilot/locks/altitude</property>
					<value type="string"></value>
				</equals>
			</not>
			<not>
				<equals>
					<property>autopilot/locks/altitude</property>
					<value type="string">agl-hold</value>
				</equals>
			</not>
			<not>
				<equals>
					<property>autopilot/locks/altitude</property>
					<value type="string">altitude-hold</value>
				</equals>
			</not>
		</and>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>alt.hold.yellow</object-name>
	<condition>
		<equals>
			<property>autopilot/locks/altitude</property>
			<value type="string">altitude-hold</value>
		</equals>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>agl.hold.yellow</object-name>
	<condition>
		<equals>
			<property>autopilot/locks/altitude</property>
			<value type="string">agl-hold</value>
		</equals>
	</condition>
 </animation>

